rating(){
  margin: 0;
  padding: 0;
  li {
    padding: 1px;
    color: rgba($palette-alpha-rating, 0.25);
    text-align: center;
    font-weight: bold;
    font-size: 18px;
    list-style-type: none;
    display: inline-block;
    cursor: pointer;
    &.filled {
      color: $palette-alpha-rating
    }
  }
}
.rating{
  rating()
}